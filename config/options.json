                    "color": "#FF8800",
                    "width": 3
                },
                "size": {
                    "width": 0.95,
                    "height": 0.95
                },
                "position": {
                    "x": 0,
                    "y": 0
                },
                "items": {
                    
                },
                "board": {
                    
                },
                "matter": {
                }
            }, 
            "types": {
                "zig_zag": {
